{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "react-starter",
  "engines": {
    "npm": ">=4",
    "node": ">=6"
  },
  "scripts": {
    "prebuild:vendor:prod": "npm run clean:build:vendor",
    "prebuild:vendor:dev": "npm run clean:build:vendor",
    "prebuild:app:prod": "npm run clean:build:app",
    "prebuild:app:dev": "npm run clean:build:app",
    "clean:build:app": "del-cli public/* !public/dll",
    "clean:build:vendor": "del-cli public/dll/vendor",
    "build:vendor:prod": "cross-env NODE_ENV=production babel-node node_modules/webpack/bin/webpack.js --config webpack/production/vendor.config.js --color --progress",
    "build:vendor:dev": "cross-env NODE_ENV=development babel-node node_modules/webpack/bin/webpack.js --config webpack/development/vendor.config.js --color --progress",
    "build:app:prod": "cross-env NODE_ENV=production babel-node node_modules/webpack/bin/webpack.js --config webpack/production/app.config.js --color --progress",
    "build:app:dev": "cross-env NODE_ENV=development babel-node node_modules/webpack/bin/webpack.js --config webpack/development/app.config.js --color --progress",
    "build:prod": "npm run build:vendor:prod && npm run build:app:prod",
    "build:dev": "npm run build:vendor:dev && npm run build:app:dev",
    "lint:stylelint": "stylelint --ignore-path .gitignore",
    "lint:eslint": "eslint --ignore-path .gitignore --fix",
    "lint:js": "npm run lint:eslint -- .",
    "lint:css": "npm run lint:stylelint -- **/*.css",
    "lint:staged": "lint-staged",
    "test:e2e": "npm run build:prod && babel-node scripts/e2e.js",
    "test:unit": "babel-node node_modules/karma/bin/karma start karma.conf.js",
    "test:ci": "npm run lint:js && npm run lint:css && npm run test:unit && npm run test:e2e",
    "test:precommit": "npm run lint:staged && npm run test:unit && npm run test:e2e",
    "start:dev": "npm run build:vendor:dev && cross-env NODE_ENV=development babel-node scripts/start-dev.js",
    "start:prod": "npm run build:prod && npm run start:server:prod",
    "start:server:prod": "cross-env NODE_ENV=production babel-node server/index.js",
    "start:api": "babel-node api/index.js",
    "precommit": "npm run test:precommit"
  },
  "lint-staged": {
    "*.css": [
      "stylefmt",
      "lint:stylelint",
      "git add"
    ],
    "*.js": [
      "lint:eslint",
      "git add"
    ]
  },
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "author": "pavel06081991",
  "license": "ISC",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-dynamic-import-node": "^1.0.1",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.22.0",
    "babili-webpack-plugin": "0.0.11",
    "chai": "^3.5.0",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "del-cli": "^0.2.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.11.0",
    "html-webpack-plugin": "^2.26.0",
    "husky": "^0.13.3",
    "inline-chunk-manifest-html-webpack-plugin": "^0.2.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "json-server": "^0.9.6",
    "karma": "^1.4.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "lint-staged": "^3.2.6",
    "mocha": "^3.2.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-loader": "^1.2.2",
    "pug": "^2.0.0-beta11",
    "pug-loader": "^2.3.0",
    "selenium-standalone": "^6.0.1",
    "sinon": "^2.1.0",
    "style-loader": "^0.16.1",
    "stylefmt": "^5.1.1",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "wdio-mocha-framework": "^0.5.8",
    "webdriverio": "^4.6.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "axios": "^0.16.0",
    "babel-polyfill": "^6.20.0",
    "compression": "^1.6.2",
    "express": "^4.14.1",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "material-ui": "^0.17.1",
    "normalize.css": "^6.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.0.2",
    "react-intl": "^2.2.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-router-scroll": "^0.4.1",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-form": "^6.6.1",
    "redux-form-material-ui": "^4.1.3",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.14.4",
    "reselect": "^3.0.0"
  }
}
